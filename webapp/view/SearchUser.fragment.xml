<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Users Found"
		title="Select Users"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path: '/Users'
		}" >
		<DisplayListItem label = "{USER_NAME}" value = "{USER_ID}"/>
	</SelectDialog>
</core:FragmentDefinition>