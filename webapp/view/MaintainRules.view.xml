<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="bam.controller.MaintainRules">
	<App id="maintainRules">
		<pages>
			<Page id="maintainRulePage" title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack" enableScrolling="false" class="marginBoxContent">
				<content>
					<TileContainer id="tlCntRuleHome" height="100%" width="100%" allowAdd="true" tiles="{path: '/ruleTiles'}">
						 <StandardTile title="{title}" tooltip="{toolTipText}" icon="{icon}" activeIcon="{icon}" infoState="Success" class="sapUiLargeMarginEnd"  press="onGoToCuSubCuAssignment"/>
					</TileContainer>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>