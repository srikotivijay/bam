<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="bam.controller.EditAttributesMultiple">
	<App id="editAttributesMultiple">
		<pages>
			<Page title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack">
				<content>
				<Text id="lblGMID" text="{i18n>gmidcountry}" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom pageHeaderFont boldFont"/>	
				<Text id="lblGMIDCountryCount" text="{/GMID_COUNTRY_COUNT}" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom pageHeaderFont boldFont"/>	
				<f:Form editable="true" id="formEditAttributeMultiple">
				    <f:formContainers>
				        <f:FormContainer title="{i18n>editAttributes}" id="fConEditAttributes">
				            <f:formElements>
				                <f:FormElement label="{i18n>storedCurrency}" id="felmCurrency" visible="{path: '/SHOW_CURRENCY', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlStoredCurrency" name="ddlStoredCurrency" change="onChange" items="{path: '/STORED_CURRENCY_LIST'}" selectedKey="{/CURRENCY_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>ibpRelevancy}" id="felmIBPRelevancy" visible="{path: '/SHOW_IBP_RELEVANCY', formatter: '.showControl'}">
				                    <f:fields>
				                       <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlIBPRelevancyFlag" name="ddlIBPRelevancyFlag" change="onChange" items="{path: '/IBP_RELEVANCY_LIST'}" selectedKey="{/IBP_RELEVANCY_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>nettingDefault}" id="felmNettingDefault" visible="{path: '/SHOW_NETTING_DEFAULT', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlNettingDefaultFlag" name="ddlNettingDefaultFlag" change="onChange" items="{path: '/NETTING_DEFAULT_LIST'}" selectedKey="{/NETTING_DEFAULT_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>quadrant}" id="felmQuadrant" visible="{path: '/SHOW_QUADRANT', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlQuadrant" name="ddlQuadrant" change="onChange" items="{path: '/QUADRANT_LIST'}" selectedKey="{/QUADRANT_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>channel}" id="felmChannel" visible="{path: '/SHOW_CHANNEL', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlChannel" name="ddlChannel" change="onChange" items="{path: '/CHANNEL_LIST'}" selectedKey="{/CHANNEL_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>marketDefault}" id="felmMarketDefaultFlag" visible="{path: '/SHOW_MARKET_DEFAULT', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlMarketDefaultFlag" name="ddlMarketDefaultFlag" change="onChange" items="{path: '/MARKET_DEFAULT_LIST'}" selectedKey="{/MARKET_DEFAULT_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>consensusDefault}" id="felmConsensusDefaultFlag" visible="{path: '/SHOW_CONSENSUS_DEFAULT', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlConsensusDefaultFlag" name="ddlConsensusDefaultFlag" change="onChange" items="{path: '/CONSENSUS_DEFAULT_LIST'}" selectedKey="{/CONSENSUS_DEFAULT_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>supplySystemFlag}" id="felmSupportSystemFlag" visible="{path: '/SHOW_SUPPLY_SYSTEM_FLAG', formatter: '.showControl'}">
				                    <f:fields>
				                        <Select xmlns:sap.ui.core="sap.ui.core" width="40%" id="ddlSupportSystemFlag" name="ddlSupportSystemFlag" change="onChange" items="{path: '/SUPPORT_SYSTEM_FLAG_LIST'}" selectedKey="{/SUPPLY_SYSTEM_FLAG_CODE_ID}">
		                            		<sap.ui.core:ListItem key="{ID}" text="{LABEL}"/>
										</Select>
				                    </f:fields>
				                </f:FormElement>
				                <f:FormElement label="{i18n>demandAtt1}" id="felmDemandAtt1" visible="{path: '/SHOW_DEMAND_ATTRIBUTE1', formatter: '.showControl'}">
				                    <f:fields>
				                        <Input id="txtDemandAtt1" name="txtDemandAtt1" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/DEMAND_ATTRIBUTE1}"/>
				                        <CheckBox id="chkDemandAtt1" name="chkDemandAtt1" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_DEMAND_ATTRIBUTE1}" tooltip="{i18n>updateNullTooltip}" />
				                    </f:fields>
				                </f:FormElement>
					            <f:FormElement label="{i18n>demandAtt2}" id="felmDemandAtt2" visible="{path: '/SHOW_DEMAND_ATTRIBUTE2', formatter: '.showControl'}">
					                    <f:fields>
					                        <Input id="txtDemandAtt2" name="txtDemandAtt2" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/DEMAND_ATTRIBUTE2}"/>
					                        <CheckBox id="chkDemandAtt2" name="chkDemandAtt2" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_DEMAND_ATTRIBUTE2}" tooltip="{i18n>updateNullTooltip}"/>
					                    </f:fields>
					            </f:FormElement>
					            <f:FormElement label="{i18n>mktAtt1}" id="felmMktAtt1" visible="{path: '/SHOW_MARKETING_ATTRIBUTE1', formatter: '.showControl'}">
					                    <f:fields>
					                        <Input id="txtMktAtt1" name="txtMktAtt1" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/MARKETING_ATTRIBUTE1}"/>
					                        <CheckBox id="chkMktAtt1" name="chkMktAtt1" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_MARKETING_ATTRIBUTE1}" tooltip="{i18n>updateNullTooltip}"/>
					                    </f:fields>
					            </f:FormElement>
					            <f:FormElement label="{i18n>mktAtt2}" id="felmMktAtt2" visible="{path: '/SHOW_MARKETING_ATTRIBUTE2', formatter: '.showControl'}">
					                    <f:fields>
					                        <Input id="txtMktAtt2" name="txtMktAtt2" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/MARKETING_ATTRIBUTE2}"/>
					                         <CheckBox id="chkMktAtt2" name="chkMktAtt2" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_MARKETING_ATTRIBUTE2}" tooltip="{i18n>updateNullTooltip}"/>
					                    </f:fields>
					            </f:FormElement>
					            <f:FormElement label="{i18n>supplyAtt1}" id="felmSupplyAtt1" visible="{path: '/SHOW_SUPPLY_ATTRIBUTE1', formatter: '.showControl'}">
					                    <f:fields>
					                        <Input id="txtSupplyAtt1" name="txtSupplyAtt1" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/SUPPLY_ATTRIBUTE1}"/>
					                        <CheckBox id="chkSupplyAtt1" name="chkSupplyAtt1" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_SUPPLY_ATTRIBUTE1}" tooltip="{i18n>updateNullTooltip}"/>
					                    </f:fields>
					            </f:FormElement>
					            <f:FormElement label="{i18n>supplyAtt2}" id="felmSupplyAtt2" visible="{path: '/SHOW_SUPPLY_ATTRIBUTE2', formatter: '.showControl'}">
					                    <f:fields>
					                        <Input id="txtSupplyAtt2" name="txtSupplyAtt2" maxLength = "50" liveChange="onChange" valueLiveUpdate="true" width="97%" value="{/SUPPLY_ATTRIBUTE2}"/>
					                        <CheckBox id="chkSupplyAtt2" name="chkSupplyAtt2" text="{i18n>updateNull}" select="onChecked" selected="{/NULLIFY_SUPPLY_ATTRIBUTE2}" tooltip="{i18n>updateNullTooltip}"/>
					                    </f:fields>
					            </f:FormElement>
				            </f:formElements>
				        </f:FormContainer>
				    </f:formContainers>
				    <f:layout>
				        <f:ResponsiveGridLayout id="fResGridEditAttributes"/>
				    </f:layout>
				</f:Form>
				<Toolbar width="100%" id="tbrSubmit">
				     <content>
				        <ToolbarSpacer id="tbrSpSubmit"/>
						<Button text="{i18n>submit}" width="100px" id="btnSubmit" press="onSubmit"/>
				        <Button text="{i18n>cancel}" width="100px" id="btnCancel" press="onCancel" class="sapUiLargeMarginEnd"/>
				     </content>
					</Toolbar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>