{
	"_version": "1.5.0",
	"sap.app": {
		"id": "bam",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"BAMDataService.xsodata": {
				"uri": "/ODataService/BAMDataService.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation"
					]
				}
			},
			"annotation": {
				"type": "ODataAnnotation",
				"uri": "annotations/annotation.xml",
				"settings": {
					"localUri": "annotations/annotation.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "bam.view.Home",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "bam.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "BAMDataService.xsodata",
				"preload": true
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "bam/view",
				"controlId": "home",
				"transition": "slide",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "gmidSubmission",
					"name": "gmidSubmission",
					"target": "gmidSubmission"
				},
				{
					"pattern": "gmidPlantSelection",
					"name": "gmidPlantSelection",
					"target": "gmidPlantSelection"
				},
				{
					"pattern": "maintainAttributes",
					"name": "maintainAttributes",
					"target": "maintainAttributes"
				},
				{
					"pattern": "editAttributesSingle/{editAttributesID}",
					"name": "editAttributesSingle",
					"target": "editAttributesSingle"
				},
				{
					"pattern": "editAttributesMultiple/",
					"name": "editAttributesMultiple",
					"target": "editAttributesMultiple"
				},
				{
					"pattern": "gmidPlant",
					"name": "gmidPlant",
					"target": "gmidPlant"
				},
				{
					"pattern": "gmidPlantAssignment/{gmidids}",
					"name": "gmidPlantAssignment",
					"target": "gmidPlantAssignment"
				},
				{
					"pattern": "notFound",
					"name": "notFound",
					"target": "notFound"
				},
				{
					"pattern": "accessDenied",
					"name": "accessDenied",
					"target": "accessDenied"
				},
				{
					"pattern": "auditReport",
					"name": "auditReport",
					"target": "auditReport"
				},
				{
					"pattern": "maintainRules",
					"name": "maintainRules",
					"target": "maintainRules"
				},
				{
					"pattern": "cuAssignment",
					"name": "cuAssignment",
					"target": "cuAssignment"
				},
				{
					"pattern": "addCURules",
					"name": "addCURules",
					"target": "addCURules"
				},
				{
					"pattern": "editCURules",
					"name": "editCURules",
					"target": "editCURules"
				},
				{
					"pattern": "peopleAssignment",
					"name": "peopleAssignment",
					"target": "peopleAssignment"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewId": "home",
					"transition": "flip"
				},
				"gmidSubmission": {
					"viewName": "GMIDSubmission",
					"viewId": "gmidSubmission"
				},
				"gmidPlantSelection": {
					"viewName": "PlantSelection",
					"viewId": "plantSelection"
				},
				"maintainAttributes": {
					"viewName": "MaintainAttributes",
					"viewId": "maintainAttributes"
				},
				"editAttributesSingle": {
					"viewName": "EditAttributesSingle",
					"viewId": "editAttributesSingle"
				},
				"editAttributesMultiple": {
					"viewName": "EditAttributesMultiple",
					"viewId": "editAttributesMultiple"
				},
				"gmidPlant": {
					"viewName": "GMIDPlant",
					"viewId": "GMIDPlant"
				},
				"gmidPlantAssignment": {
					"viewName": "GMIDPlantAssignment",
					"viewId": "GMIDPlantAssignment"
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"accessDenied": {
					"viewName": "AccessDenied",
					"viewId": "accessDenied"
				},
				"auditReport": {
					"viewName": "AuditReport",
					"viewId": "auditReport"
				},
				"maintainRules": {
					"viewName": "MaintainRules",
					"viewId": "maintainRules"
				},
				"cuAssignment": {
					"viewName": "CUAssignment",
					"viewId": "cuAssignment"
				},
				"addCURules": {
					"viewName": "AddCURules",
					"viewId": "addCURules"
				},
				"editCURules": {
					"viewName": "EditCURules",
					"viewId": "editCURules"
				},
				"peopleAssignment": {
					"viewName": "PeopleAssignment",
					"viewId": "peopleAssignment"
				}
			}
		}
	}
}